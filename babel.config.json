{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [
          {
            "rootPathPrefix": "~/",
            "rootPathSuffix": "src"
          },
          {
            "rootPathPrefix": "?/",
            "rootPathSuffix": "test"
          }
        ]
      }
    ]
  ]
}
